extends base

block content
  .page
    .container-build
      .head-box
        .banner
          div.search-fa.col-md-6.col-md-offset-3
            form.search(method="get")
              input.form-control.search-input(type="text" name="search")
    .container
      .col-md-10
        each post in posts
          li #{post.title}
